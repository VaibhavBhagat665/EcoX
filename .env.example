# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_firebase_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Builder.io Configuration
VITE_BUILDER_API_KEY=your_builder_api_key_here

# AI Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Vercel Configuration (for deployment)
VERCEL_TOKEN=your_vercel_token_here
VERCEL_ORG_ID=your_vercel_org_id
VERCEL_PROJECT_ID=your_vercel_project_id

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/ecox_db

# Application Configuration
NODE_ENV=development
PORT=5000

# External APIs
WEATHER_API_KEY=your_weather_api_key
CARBON_INTERFACE_API_KEY=your_carbon_interface_key

# Security
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Analytics
GOOGLE_ANALYTICS_ID=your_ga_id
MIXPANEL_TOKEN=your_mixpanel_token

# Email Configuration (optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password

# Social Media API Keys (optional)
TWITTER_API_KEY=your_twitter_api_key
FACEBOOK_APP_ID=your_facebook_app_id

# Monitoring
SENTRY_DSN=your_sentry_dsn
LOGROCKET_APP_ID=your_logrocket_id

# Feature Flags
ENABLE_VOICE_ASSISTANT=true
ENABLE_3D_SCENE=true
ENABLE_COMMUNITY_FEATURES=true
ENABLE_AI_CHAT=true
ENABLE_ANALYTICS=true
